{
  "ConnectionStrings": {
    "DefaultConnection": ""
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.EntityFrameworkCore": "Information",
      "Microsoft.EntityFrameworkCore.Database.Command": "Information"
    }
  },
  "Security": {
    "RequireHttps": false,
    "EnableCors": true
  },
  "Cors": {
    "AllowedOrigins": [
      "https://legacyprocs-frontend.onrender.com"
    ]
  },
  "Api": {
    "EnableSwagger": true,
    "DetailedErrors": false,
    "MaxPageSize": 50,
    "DefaultPageSize": 10
  }
}
